<body>
<!-- Styles -->

<!-- MODAL -->
<div class="modal fade" id="miModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel" style="font-weight: bold">Información</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="mensaje_modal"></div>
            </div>
        </div>
    </div>
</div><!-- END MODAL -->

<div id="dv_mensaje" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dv_mesaje" aria-hidden="true"
     data-backdrop="static" data-keyboard="false">
    <div style="background-color:#ffffff;width:390px;margin:auto">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <div class="offset-11">
                        <button type="button" class="btn btn-danger" data-dismiss="modal" aria-label="Close"
                                style="width: 35px"><i aria-hidden="true" style="color: white; opacity: 1">&times;</i>
                        </button>
                    </div>
                </div>
                <div class="modal-body">
                    <div id="mensaje"></div>
                </div>
                <div class="modal-footer">
                    <div class="offset-3">
                        <button type="button" class="btn btn-success" data-dismiss="modal" aria-label="Close"
                                onclick="$('#dv_mensaje').modal('hide');">ACEPTAR
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="modalEliminar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header modalHeader">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"
                                                                                                  style="color: white; opacity: 1">&times;</span>
                </button>
                <i class="fa fa-question-circle fa-lg iconoHeader" aria-hidden="true"></i>
                <h4 class="modal-title" id="myModalLabel">Confirmar Eliminaci&oacute;n</h4>
            </div>
            <div class="modal-body">

                <label class="control-label" id="mensajeModal"></label>

                <br/>
                <br/>

                <label class="control-label"> La eliminaci&oacute;n de este registro podría causar inestabilidad en el
                    sistema.</label>

                <label class="control-label"> &iquest;Est&aacute;s seguro de eliminar el a&ntilde;o
                    seleccionado?</label>

            </div>
            <div class="modal-footer">
                <span hidden="hidden" id="itemId"></span>
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                <button id="btn_eliminar_item" type="button" class="btn btn-danger" onclick="deleteConfirmed(1);">
                    Eliminar
                </button>
            </div>
        </div>
    </div>
</div>
<div id="app">
    <div class="wrapper">
        <!-- Sidebar  -->
        <nav id="sidebar">
            <div class="sidebar-header">
                <a class="navbar-brand" href="#">
                    <img class="logo" id="logoEmp1" src="{{ asset('./dog-logo.jpg') }}" alt="">
                </a>
            </div>

            <ul class="list-unstyled components">
                <li class="active">
