                        <i class="fa fa-file-text-o"></i>
                        <b>MASCOTAS</b>
                    </a>
                    <ul class="collapse list-unstyled" id="homeSubmenu">
                        <li>
                            <a href="">CONTROL GESTION AIC </a>
                        </li>
                        <li>
                            <a href="">CONTROL ACREDITACION VEHICULAR
                            </a>
                        </li>
                        <li>
                            <a href="">CONTROL GESTION INFRACCIONES</a>
                        </li>
                        <li>
                            <a href="">CONTROL GESTION VIAL (CARGAS, SOBREDIMENSIONES)</a>
                        </li>
                        <li>
                    </ul>
                </li>
                <li >
                    <a href="#homeSubmenu" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle">
                        <i class="fa fa-file-text-o"></i>
                        <b>CLIENTE</b>
                    </a>
                    <ul class="collapse list-unstyled" id="homeSubmenu">
                        <li>
                            <a href="">FLUJO DE CONTROL VEHICULAR </a>
                        </li>
                        <li>
                            <a href="">FLUJO DE CONTROL PERSONAS</a>
                        </li>
                    </ul>
                </li>
            </ul>

        </nav>

        <!-- Page Content  -->
        <div id="content">
            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container-fluid">

                    <button type="button" id="sidebarCollapse" class="btn btn-info">
                        <i class="fa fa-align-left"></i>
                        <span></span>
                    </button>

                </div>
            </nav>
            {% block body %}
            {% endblock %}
        </div>
    </div>
    </div>
</div>
</div>
